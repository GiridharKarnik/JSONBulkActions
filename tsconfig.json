{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "jsx": "react",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "src",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": [
        "node_modules/*"
      ],
      "src": [
        "src"
      ]
    }
  },
  "include": [
    "src"
  ]
}
